{
  "name": "solid-7css",
  "type": "module",
  "version": "1.0.0",
  "description": "SolidJS wrapper components for 7.css Windows 7 UI framework",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "style": "dist/7.css",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./css": "./dist/7.css",
    "./style.css": "./dist/7.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "bunx --bun vite",
    "build": "bunx --bun vite build && bunx tsc --emitDeclarationOnly",
    "build:lib": "bunx --bun vite build --mode lib && bunx tsc --emitDeclarationOnly",
    "preview": "bunx --bun vite preview",
    "typecheck": "bunx tsc --noEmit",
    "lint": "bunx eslint src/**/*.{ts,tsx}",
    "prepublishOnly": "bun run build:lib",
    "check-updates": "bun scripts/check-updates.js",
    "update-7css": "bun scripts/check-updates.js --auto-update && bun install"
  },
  "keywords": [
    "solidjs",
    "7css",
    "windows7",
    "ui",
    "components",
    "css",
    "framework"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/solid-7css.git"
  },
  "peerDependencies": {
    "solid-js": "^1.8.0"
  },
  "dependencies": {
    "7.css": "^0.21.1",
    "clsx": "^2.1.1"
  },
  "devDependencies": {
    "@solidjs/testing-library": "^0.8.10",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "globals": "^16.3.0",
    "solid-js": "^1.9.7",
    "typescript": "^5.9.2",
    "vite": "^7.0.6",
    "vite-plugin-solid": "^2.11.8"
  }
}
